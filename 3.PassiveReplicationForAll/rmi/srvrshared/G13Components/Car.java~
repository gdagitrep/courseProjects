// -------------------------------// adapted from Kevin T. Manley// CSE 593// -------------------------------package G13Components;import TxSystem.*;public class Car extends ReservableItem{		public Car( String location, int count, int price )		{			super( location, count, price );		}		public String getKey()		{			return Car.getKey( getLocation() );		}		public static String getKey( String location )		{			String s = "car-" + location  ;			return s.toLowerCase();		}		public DataObj getTxDataObj(int txId, int lockType)		{			return new DataObj(txId, getKey(), lockType);		}}